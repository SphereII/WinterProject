<configs>

	<!-- Reset the block IDs for the blocks. We'll replace them with our new choices -->
	<config name="blocks">	
			
		<!-- Wood table to be replaced with Christmas tree -->
		<set xpath="/blocks/block[@id='1416']/@id">*</set>
		
		<!-- OldChair - to be replaced with christmas gifts -->
		<set xpath="/blocks/block[@id='312']/@id">*</set>
		
		<!-- Trash Can with Xmas Gift -->
		<set xpath="/blocks/block[@id='335']/@id">*</set>
	</config>
		<!-- Closing the blocks and reopening, so that SDX will see the new wild cards -->
	<config name="blocks">	
		<append xpath="/blocks">	
			<block id="335" name="xmasGift">
				<property name="Class" value="Loot" />
				<property name="LootList" value="8" />
				<property name="CustomIcon" value="xmasGift" />
				<property name="Material" value="paper" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#Gifts?GiftPrefab" />   <!-- Present by holtkamp is licensed under CC Attribution  -->
				<property name="Collide" value="movement,melee,bullet,arrow,rocket" />
				<property name="DescriptionKey" value="xmasGift" />
				<drop event="Destroy" count="0" />
				<drop event="Destroy" name="paper" count="1" prob="1" />
				<drop event="Fall" name="paper" count="0" prob="1" stick_chance="1" />
				<drop event="Fall" name="paper" count="1" prob=".75" stick_chance="1" />
			</block>
			
			<!-- This will replace the oldChair -->
			<block id="312" name="xmasGifts">
				<property name="Class" value="Loot" />
				<property name="LootList" value="8" />
				<property name="CustomIcon" value="xmasGifts" />
				<property name="Material" value="paper" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#Gifts?GiftsPrefab" />  <!-- Stack of Christmas Gifts by Damco is licensed under CC Attribution  -->
				<property name="Collide" value="movement,melee,bullet,arrow,rocket" />
				<property name="DescriptionKey" value="xmasGifts" />
				<property name="OnlySimpleRotations" value="true" />
				<drop event="Destroy" count="0" />
				<drop event="Destroy" name="paper" count="5" prob="1" />
				<drop event="Fall" name="paper" count="0" prob="1" stick_chance="1" />
				<drop event="Fall" name="paper" count="5" prob=".75" stick_chance="1" />
			</block>
			<!-- This will replace the woodTable -->
			<block id="1416" name="xmasTree">
				<property name="CustomIcon" value="xmasTree" />
				<property name="Material" value="treeWoodSmall" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#XmasTree?XmasTreePrefab" />
				<property name="CanPickup" value="true" />
				<property name="Collide" value="movement,melee,bullet,arrow,rocket" />
				<property name="DescriptionKey" value="xmasTree" />
				<drop event="Destroy" count="0" />
				<drop event="Harvest" name="wood" count="10" tool_category="harvestingTools" />
				<property name="MultiBlockDim" value="1,3,1" />
				<property name="CanPlayersSpawnOn" value="false" />
				<property name="ParticleOnDeath" value="treeGib_small_dust" />
			</block>
			<block id="" name="icicle"> 
				<property name="Material" value="snow" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#WinterBlocks?IciclePrefab"/> 
				<property name="CanPickup" value="false" />
				<property name="CustomIcon" value="icicle" />
				<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
				<drop event="Harvest" name="snowBall" count="3" tool_category="harvestingTools"/>
				<drop event="Destroy" count="0"/>
				<drop event="Fall" name="snow" count="1" prob="0.1" stick_chance="1"/>
				<drop event="Fall" name="snowBall" count="3" prob="0.187" stick_chance="0"/>
			</block>
			<block id="" name="icicles"> 
				<property name="Material" value="snow" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#WinterBlocks?IciclesPrefab"/> 
				<property name="CanPickup" value="false" />
				<property name="CustomIcon" value="icicle" />
				<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
				<drop event="Harvest" name="snowBall" count="10" tool_category="harvestingTools"/>
				<drop event="Destroy" count="0"/>
				<drop event="Fall" name="snow" count="1" prob="0.1" stick_chance="1"/>
				<drop event="Fall" name="snowBall" count="10" prob="0.187" stick_chance="0"/>
			</block>
			
			<block id="" name="iceBlock"> 
				<property name="Material" value="snow" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#WinterBlocks?IceBlockPrefab"/> <!-- Ice Blocks by vanquish99 is licensed under CC Attribution -->
				<property name="CanPickup" value="false" />
				<property name="CustomIcon" value="iceblock" />
				<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
				<property name="ImposterExchange" value="imposterBlock" param1="62" />
				<property name="DropScale" value="2" />
				<property class="RepairItems">
					<property name="snowBall" value="10" />
				</property>
				<drop event="Harvest" name="snowBall" count="10" tool_category="harvestingTools" />
				<drop event="Destroy" count="0" />
				<drop event="Fall" name="snow" count="1" prob="0.25" stick_chance="1" />
				<drop event="Fall" name="snowBall" count="10" prob="0.187" stick_chance="0" />
				<property name="EconomicValue" value="5" />
				<property name="EconomicBundleSize" value="1" />
				<property name="SellableToTrader" value="false" />
			</block>
			<block id="" name="snowBlock"> 
				<property name="Material" value="snow" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#WinterBlocks?SnowBlockPrefab"/> <!-- Ice Blocks by vanquish99 is licensed under CC Attribution -->
				<property name="CanPickup" value="false" />
				<property name="CustomIcon" value="snowblock" />
				<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
				<property name="ImposterExchange" value="imposterBlock" param1="62" />
				<property name="DropScale" value="2" />
				<property class="RepairItems">
					<property name="snowBall" value="10" />
				</property>
				<drop event="Harvest" name="snowBall" count="10" tool_category="harvestingTools" />
				<drop event="Destroy" count="0" />
				<drop event="Fall" name="snow" count="1" prob="0.25" stick_chance="1" />
				<drop event="Fall" name="snowBall" count="10" prob="0.187" stick_chance="0" />
				<property name="EconomicValue" value="5" />
				<property name="EconomicBundleSize" value="1" />
				<property name="SellableToTrader" value="false" />
			</block>
			<block id="" name="rabbitBlock">
				<property name="Material" value="cloth" />
				<property name="Shape" value="ModelEntity" /> <!-- Rabbit Christmas by Chaitanya Krishnan is licensed under CC Attribution -->
				<property name="Model" value="#Rabbit?RabbitBlockPrefab" />		
				<property name="IsDecoration" value="true" />
				<property name="ImposterExclude" value="true" />
				<property name="Place" value="TowardsPlacer" />
				<property name="Weight" value="30" />
				<property name="CanPickup" value="true" param1="rabbitItem" />
				<property name="DescriptionKey" value="" />
				<drop event="Destroy" name="cloth" count="1,3" prob="1" />
			</block>
			<block id="" name="wreath">
				<property name="CustomIcon" value="xmasTree" />
				<property name="Material" value="treeWoodSmall" />
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#Wreath?WreathPrefab" />
				<property name="IsDecoration" value="true" />
				<property name="ImposterExclude" value="true" />
				<property name="CanPickup" value="true" />
				<property name="Collide" value="movement,melee,bullet,arrow,rocket" />
				<property name="DescriptionKey" value="wreath" />
				<drop event="Destroy" count="0" />
				<drop event="Harvest" name="wood" count="1" tool_category="harvestingTools" />
				<property name="ParticleOnDeath" value="treeGib_small_dust" />
			</block>
			<block id="" name="stockings">
				<property name="Material" value="cloth" />
				<property name="Shape" value="ModelEntity" /> 
				<property name="Model" value="#Stockings?Stockings" />		
				<property name="IsDecoration" value="true" />
				<property name="ImposterExclude" value="true" />
				<property name="Weight" value="30" />
				<property name="CanPickup" value="true" />
				<property name="DescriptionKey" value="" />
				<drop event="Destroy" name="cloth" count="1,3" prob="1" />
			</block>
		</append>
	</config>
	<config name="items">
		<append xpath="/items">
				<item id="" name="rabbitItem">
				<property name="DescriptionKey" value="toysDesc" />
				<property name="Meshfile" value="#Rabbit?RabbitItemPrefab" /> <!-- Rabbit Christmas by Chaitanya Krishnan is licensed under CC Attribution -->
				<property name="DropMeshfile" value="#Rabbit?RabbitItemPrefab" />
				<property name="Material" value="cloth" />
				<property name="HoldType" value="31" />
				<property name="Stacknumber" value="1" />		
				<property class="Action1">
					<property name="Class" value="PlaceAsBlock" />
					<property name="Delay" value="0.1" />
					<property name="Blockname" value="rabbitBlock" />
				</property>
				<property name="Group" value="Basics,Decor/Miscellaneous" />
				<property name="EconomicValue" value="25" />
			</item>
		</append>	  
	</config>
	<config name="recipes">
		<append xpath="/recipes" >
			<recipe name="xmasGift" count="1" craft_area="workbench">
				<ingredient name="paper" count="2" />
				<ingredient name="glue" count="1" />
			</recipe>			
			<recipe name="xmasGifts" count="1" craft_area="workbench">
				<ingredient name="paper" count="10" />
				<ingredient name="glue" count="2" />
			</recipe>			
		</append>
	</config>
</configs>